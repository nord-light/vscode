  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9099AB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#6B8B4F"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#1D7A79",
        "fontStyle": "bold"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#B8941E"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#8B5E84"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#8B5E84"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#B8941E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.nan"
      ],
      "settings": {
        "foreground": "#8B5E84"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#3B5E85",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.relational",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#4B6C8C",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3B5E85",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class",
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#1D7A79"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.type.interface"
